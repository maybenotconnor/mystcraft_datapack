{
  "model": {
    "type": "minecraft:select",
    "property": "minecraft:custom_model_data",
    "index": 0,
    "cases": [
      {
        "when": "linkbook",
        "model": {
          "type": "minecraft:model",
          "model": "mystcraft:item/linkbook"
        }
      },
      {
        "when": "playerbook",
        "model": {
          "type": "minecraft:model",
          "model": "mystcraft:item/playerbook"
        }
      },
      {
        "when": "deathbook",
        "model": {
          "type": "minecraft:model",
          "model": "mystcraft:item/skullbook"
        }
      },
      {
        "when": "tornbook",
        "model": {
          "type": "minecraft:model",
          "model": "mystcraft:item/tornbook"
        }
      }
    ],
    "fallback": {
      "type": "minecraft:model",
      "model": "minecraft:item/book"
    }
  }
}